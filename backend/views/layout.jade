- var listSize=5

doctype html
html
    block vars
    head
        title= title
        link(rel='stylesheet', href='/stylesheets/style.min.css')
        link(rel='shortcut icon', href='/images/favicon.ico')
        meta(name='viewport', content='initial-scale=1.0')
        script(src='/app.min.js')
        script(src='http://maps.google.com/maps/api/js?sensor=false')
    body
        div#page.container
            h1= title
            div.row
                div(class='col-md-#{12-listSize}')
                    block content
                div(class='col-md-#{listSize}')
                    div#list
                        block list
            div.well
                span !{md(__i('[Fork me](%s) or [create feature request](%s) on GitHub. [About author](%s).', 'https://github.com/horejsek/lunchtimeandel', 'https://github.com/horejsek/lunchtimeandel/issues', 'http://www.horejsek.com'))}
                span #{__i('I would appreciate any tip (you know – servers and domains are not cheap…) Thank you!')}
                    span &nbsp;
                    form(action='https://www.paypal.com/cgi-bin/webscr', method='post', target='_top', style='display:inline')
                        input(type='hidden', name='cmd', value='_s-xclick')
                        input(type='hidden', name='encrypted', value='-----BEGIN PKCS7----------END PKCS7-----')
                        input(type='image', src='https://www.paypalobjects.com/en_US/i/btn/btn_donate_LG.gif', border='0', name='submit', alt='PayPal - The safer, easier way to pay online!')
        block end
        script(type='text/javascript').
            var _gaq = _gaq || [];
            _gaq.push(['_setAccount', 'UA-37406706-1']);
            _gaq.push(['_setDomainName', 'lunchtimeandel.cz']);
            _gaq.push(['_trackPageview']);
            (function() {
                var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
                ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
            })();